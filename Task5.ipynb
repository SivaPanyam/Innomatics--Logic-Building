   "source": [
    "# Error Message Detector\n",
    "# Detect and count error messages from system logs\n",
    "\n",
    "# System logs\n",
    "logs = [\"INFO\", \"ERROR\", \"WARNING\", \"ERROR\"]\n",
    "\n",
    "# Count ERROR entries\n",
    "error_count = logs.count(\"ERROR\")\n",
    "\n",
    "# Print the total error count\n",
    "print(f\"Total ERROR count: {error_count}\")"
   ]
  }
